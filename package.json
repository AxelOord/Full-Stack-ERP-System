    "build-prod": "nx run backend:container --configuration=production && nx run frontend:container --configuration=production",
    "tag-latest": "docker tag backend:latest serajevo/backend:latest && docker tag frontend:latest serajevo/frontend:latest",
    "tag-version": "docker tag backend:latest serajevo/backend:$VERSION && docker tag frontend:latest serajevo/frontend:$VERSION",
    "push-latest": "npm run tag-latest && docker push serajevo/backend:latest && docker push serajevo/frontend:latest",
